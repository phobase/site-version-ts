{
  "compilerOptions": {
    "module": "esnext",
    "target": "es2017",
    "skipLibCheck": true,
    "declaration": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  }
}